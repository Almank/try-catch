{
  "name": "@almank/try-catch",
  "version": "1.0.0",
  "description": "Try/Catch made simple.",
  "author": "Christoffer Almankaas <chrisa.a87@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "library",
    "try/catch",
    "try",
    "catch",
    "error",
    "handling",
    "async",
    "await",
    "javascript",
    "typescript"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "test": "ava"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@ava/babel": "1.0.1",
    "@babel/preset-typescript": "7.10.4",
    "@types/node": "13.13.19",
    "ava": "3.12.1",
    "prettier": "2.1.1",
    "rollup": "2.26.11",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.27.2",
    "typedoc": "0.17.0-3",
    "typescript": "4.0.2"
  },
  "prettier": {
    "$schema": "http://json.schemastore.org/prettierrc",
    "tabWidth": 2,
    "endOfLine": "lf",
    "arrowParens": "avoid",
    "trailingComma": "none"
  },
  "ava": {
    "verbose": true,
    "files": [
      "test/*"
    ],
    "babel": {
      "compileAsTests": [
        "test/*"
      ],
      "extensions": [
        "ts"
      ]
    }
  }
}
